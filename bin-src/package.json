{
  "name": "workenv-bin-src",
  "version": "0.0.1",
  "description": "CLI tools for development environment",
  "bin": {
    "chrome-remote-debug": "dist/dopen",
    "git-worktree-manager": "dist/gwm"
  },
  "license": "UNLICENSED",
  "private": true,
  "author": "inside-hakumai",
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsdown",
    "build:dopen": "tsdown --entry ./src/cli.tsx",
    "build:gwm": "tsdown --entry ./src/cli/gwm.ts",
    "distribute": "npm run distribute:dopen && npm run distribute:gwm",
    "distribute:dopen": "cp dist/dopen ../dist/bin/dopen",
    "distribute:gwm": "cp dist/gwm ../dist/bin/gwm",
    "start:dopen": "tsx ./src/cli.tsx",
    "start:gwm": "tsx ./src/cli/gwm.ts",
    "test": "vitest run --typecheck",
    "lint": "xo --fix src tests",
    "format": "prettier --write ."
  },
  "dependencies": {
    "ink": "^6.3.1",
    "meow": "^14.0.0",
    "react": "^19.2.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "chalk": "^5.6.2",
    "eslint-config-xo-react": "^0.28.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "ink-testing-library": "^4.0.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsdown": "^0.15.6",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "xo": "^1.2.3"
  }
}
